{
	"manifest_version": 2,
	"name": "page-action-demo",
	"version": "1.0.0",
	"description": "pageAction演示",
	"icons":
	{
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"page_action":
	{
		"default_icon": "img/icon.png",
		"default_title": "我是pageAction",
		"default_popup": "popup.html"
	},
	"permissions":
	[
		"declarativeContent",
    "tabs"
	],
	"background":
	{
    "page":"background.html"
	}
}